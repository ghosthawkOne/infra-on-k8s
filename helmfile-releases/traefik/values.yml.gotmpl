ingressRoute:
  dashboard:
    enabled: false

providers:
  kubernetesCRD:
    enabled: true
    allowCrossNamespace: true
    allowExternalNameServices: true

logs:
  general:
    level: PANIC
    format: json
  access:
    level: DEBUG
    format: json
    fields:
      headers:
        defaultmode: keep
    enabled: true

service:
  type: LoadBalancer
